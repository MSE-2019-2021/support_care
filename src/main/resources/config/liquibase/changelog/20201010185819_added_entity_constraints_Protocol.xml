<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Protocol.
    -->
    <changeSet id="20201010185819-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="therapeutic_regime_id"
                                 baseTableName="protocol"
                                 constraintName="fk_protocol_therapeutic_regime_id"
                                 referencedColumnNames="id"
                                 referencedTableName="therapeutic_regime"/>

        <addForeignKeyConstraint baseColumnNames="outcome_id"
                                 baseTableName="protocol"
                                 constraintName="fk_protocol_outcome_id"
                                 referencedColumnNames="id"
                                 referencedTableName="outcome"/>

        <addForeignKeyConstraint baseColumnNames="guide_id"
                                 baseTableName="protocol"
                                 constraintName="fk_protocol_guide_id"
                                 referencedColumnNames="id"
                                 referencedTableName="guide"/>

    </changeSet>
</databaseChangeLog>
